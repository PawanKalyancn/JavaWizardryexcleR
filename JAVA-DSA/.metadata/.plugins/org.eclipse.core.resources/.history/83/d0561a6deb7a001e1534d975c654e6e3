package pattern;

public class Pattern1 {

	static void pattern( int n) {
		
for(int i=1;i<=n;i++) {

	for(int j=1;j<=n+1-i;j++) {
				System.out.print("");
				
			}
	for(int k =1;k<=n+1;k++) {
		System.out.print("*");
		
	}
			System.out.print();

		}

	}
	
	
	public static void main(String[] args) {
		
		pattern(5);
	}

}
